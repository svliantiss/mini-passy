{
	"name": "@mini-passy/sdk",
	"version": "0.1.3",
	"description": "Lightweight AI gateway SDK for routing LLM requests",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"README.md",
		".env.example"
	],
	"bin": {
		"mini-passy": "dist/index.js"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"clean": "rm -rf dist",
		"dev": "tsx src/index.ts",
		"start": "node dist/index.js",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"dotenv": "^16.4.0"
	},
	"devDependencies": {
		"@types/node": "^20.0.0",
		"tsx": "^4.0.0",
		"typescript": "^5.0.0"
	},
	"keywords": [
		"ai",
		"llm",
		"gateway",
		"openai",
		"anthropic",
		"proxy",
		"routing"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/svliantiss/mini-passy.git"
	}
}
